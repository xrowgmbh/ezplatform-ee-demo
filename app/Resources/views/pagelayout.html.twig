<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        {% block page_head %}
            {% include '::page_head.html.twig' %}
            {% include '::page_head_style.html.twig' %}
            {% include '::page_head_script.html.twig' %}
        {% endblock %}
    </head>
    <body>
        <div id="page">
            <nav role="navigation" class="navbar">
                <div class="container">
                    <div class="visible-xs-block">
                        <div id="main-nav" class="collapse navbar-collapse">
                            {{ render(controller('AppBundle:Menu:topMenu', {
                                'currentLocationId': currentLocation is defined ? currentLocation.id : null,
                                'template': '::page_header_menu.html.twig'
                            })) }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2 col-xs-4">
                            <div class="center-block home">
                                <a href="/">{{ 'Tasteful'|trans }}<br><span class="text-primary">{{ 'Planet'|trans }}</span></a>
                            </div>
                        </div>

                        <div class="col-md-10 col-xs-8">
                            <div class="row visible-xs-block">
                                <div class="navbar-header">
                                    <button type="button" data-target="#main-nav" data-toggle="collapse" class="navbar-toggle">
                                        <svg width="32" height="32" viewBox="0 0 1792 1792" xmlns="http://www.w3.org/2000/svg"><path d="M1664 1344v128q0 26-19 45t-45 19h-1408q-26 0-45-19t-19-45v-128q0-26 19-45t45-19h1408q26 0 45 19t19 45zm0-512v128q0 26-19 45t-45 19h-1408q-26 0-45-19t-19-45v-128q0-26 19-45t45-19h1408q26 0 45 19t19 45zm0-512v128q0 26-19 45t-45 19h-1408q-26 0-45-19t-19-45v-128q0-26 19-45t45-19h1408q26 0 45 19t19 45z"/></svg>
                                    </button>
                                </div>
                            </div>
                            <div class="row home-right hidden-xs">
                                <div class="col-md-7 col-sm-12 home-top-left text-center">
                                    {{ render(controller('AppBundle:Menu:topMenu', {
                                        'currentLocationId': currentLocation is defined ? currentLocation.id : null,
                                        'template': '::page_header_menu.html.twig'
                                    })) }}
                                </div>

                                <div class="col-md-3 hidden-sm search">
                                    {% block searchbox %}
                                        {{ render(controller('app.controller.search:searchBoxAction')) }}
                                    {% endblock %}
                                </div>

                                <div class="col-md-2 hidden-sm user-menu">
                                    {% block user %}
                                        {% include ':parts:user_menu.html.twig' %}
                                    {% endblock %}
                                </div>

                                <div class="col-sm-7 hidden-xs">
                                    {{ render(controller('AppBundle:Article:latestarticle', {
                                        template: '::page_header_latestarticle.html.twig'
                                    })) }}
                                </div>

                                <div class="col-md-3 col-sm-5 col-xs-12">
                                    <div class="row follow">
                                        <a href="//www.facebook.com/eZPublishEnterprise" class="fb">
