                                        </a>
                                    </div>
                                </div>

                                <div class="col-sm-2 col-xs-12 language-switcher hidden-sm">
                                    {{ render_esi(controller('app.controller.language_switcher:show', {
                                        'template': ':parts:language_switcher.html.twig'
                                    })) }}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>

            <main>
                <div class="container">
                    {% block ez_recommendations %}
                    {% endblock %}

                    <div class="col-sm-{{ innerColumnSize|default(12) }} main-content">
                        {% block content %}{% endblock %}
                    </div>
                </div>
            </main>

        </div>

        {% block footer %}
            {% include '::page_footer.html.twig' %}
        {% endblock %}
    </body>
</html>
